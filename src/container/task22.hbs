        <div class="card" style="width: 50rem;">
            <div class="card-body">
                <br>
                {{#with section}}
                {{#with about_us}}
                {{#with title_list}}
                <p class="text-end display-6">{{main}}</p>
                <h3 class="text-end text-wrap">{{sub}}</h3>
                {{/with}}
                <br>
                {{#with button}}
                <button type="button" class="btn btn-outline-info">{{text}}</button>
                {{/with}}
                {{/with}}
                <br>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with advantage}}
                        <div class="display-5">{{title}}</div>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with efficiency}}
                                <div class="display-4">{{title}}</div>
                                <br>
                                <h6>{{sub_title}}</h6>
                                {{description}}
                                <br>
                                <br>
                                <button type="button" class="btn btn-outline-warning">{{button}}</button>
                                {{/with}}
                            </div>
                        </div>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with innovation}}
                                <div class="display-4 text-center">{{title}}</div>
                                <h6 class="text-center">{{sub_title}}</h6>
                                <p class="text-center">{{description}}</p>
                                <button type="button" class="btn btn-outline-warning">{{button}}</button>
                                {{/with}}
                            </div>
                        </div>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with collaboration}}
                                <div class="display-4 text-end">{{title}}</div>
                                <br>
                                <h6 class="text-end">{{sub_title}}</h6>
                                <p class="text-end">{{description}}</p>
                                <button type="button" class="btn btn-outline-warning">{{button}}</button>
                                {{/with}}
                                {{/with}}
                            </div>
                        </div>
                    </div>
                </div>
                <br>
                {{#with agreement}}
                <div class="display-5">{{title}}</div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with info}}
                        <h5>{{sub_title}}</h5>
                        {{#with detail}}
                        <div class="text-wrap">{{text}}</div>
                        <br>
                        <button type="button" class="btn btn-success">{{button}}</button>
                        {{/with}}
                        {{/with}}
                        {{/with}}
                        {{/with}}
                    </div>
                </div>
            </div>
        </div>
